{
  "name": "streaming",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint -c package.json 'src/**/*.ts'",
    "fixlint": "eslint -c package.json 'src/**/*.ts' --fix",
    "test": "[ -z ${NODE_ENV+unset} ] && npx jest --coverage src/",
    "showConfig": "[ -z ${NODE_ENV+unset} ] && npx jest --showConfig",
    "build": "tsc -b -v",
    "dev": "cross-env NODE_ENV=development PORT=3000 nodemon",
    "prod": "cross-env NODE_ENV=production PORT=3000 nodemon",
    "start": "PORT=3000 NODE_ENV=development node ./dist/index.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@thirdweb-dev/sdk": "^2.0.13",
    "audit": "^0.0.6",
    "axios": "^0.26.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "encrypt-rsa": "^2.0.1",
    "envalid": "^7.1.0",
    "ethers": "^5.6.1",
    "express": "^4.17.1",
    "fix": "^0.0.6",
    "google-auth-library": "^7.3.0",
    "googleapis": "^71.0.0",
    "graceful-fs": "4.2.2",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "libphonenumber-js": "^1.9.19",
    "luxon": "^1.27.0",
    "moment": "^2.29.2",
    "mongoose": "^5.12.14",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.5",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "razorpay": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.12.2",
    "sequelize-typescript": "^1.0.0-beta.3",
    "typescript": "^4.4.3",
    "winston": "^3.3.3",
    "winston-discord-transport": "^1.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.35",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/helmet": "0.0.48",
    "@types/jest": "^28.1.3",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/luxon": "^1.27.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.14",
    "@types/node-fetch": "^2.5.10",
    "@types/pg": "^8.6.0",
    "@types/validator": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^1.3.3",
    "prettier": "^2.3.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "tslib": "^2.4.0"
  },
  "keywords": [
    "express",
    "node",
    "react",
    "backend"
  ],
  "author": "Riya",
  "private": true,
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "dist/",
      "src/services/"
    ],
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist/",
      "src/services/"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-non-null-assertion": 0,
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        1,
        "always",
        [
          "build",
          "chore",
          "docs",
          "feat",
          "fix",
          "lint",
          "refactor",
          "revert",
          "type"
        ]
      ]
    }
  }
}
